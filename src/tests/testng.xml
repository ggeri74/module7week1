<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite1" parallel="tests" thread-count="4">

    <test name="Firefox">
        <groups>
            <run>
                <include name="Firefox"/>
            </run>
        </groups>
        <classes>
            <class name="tests.TestHotelsCom"/>
        </classes>
    </test>


    <test name="Chrome">
        <groups>
            <run>
                <include name="Chrome"/>
            </run>
        </groups>
        <classes>
            <class name="tests.TestHotelsCom"/>
        </classes>
    </test>


    <test name="IE">
        <groups>
            <run>
                <include name="IE"/>
            </run>
        </groups>
        <classes>
            <class name="tests.TestHotelsCom"/>
        </classes>
    </test>


    <test name="PhantomJS">
        <groups>
            <run>
                <include name="PhantomJS"/>
            </run>
        </groups>
        <classes>
            <class name="tests.TestHotelsCom"/>
        </classes>
    </test>

</suite>
